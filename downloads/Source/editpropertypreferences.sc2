	PROCEDURE Activate
		If Not Empty (This.nPage)
			This.Frame.Pages(This.nPage).SetFocus()
			This.nPage = 0
		Endif
		
	ENDPROC

	PROCEDURE Destroy
		_TooltipTimeout = This.nOriginalToolTipTimeout
		This.oCallingForm = .Null.
		DoDefault()
		
	ENDPROC

	PROCEDURE editdefaultaccess
		Lparameters tcCaption, tcPersistKey, tcDefaultCode
		Local lcWindowName, lcACCESS_CODE, lcCode, lcFileName
		
		lcWindowName = "Default.Property" + tcCaption
		lcFileName = Sys(2023) + "\" + lcWindowName
		lcCode = Nvl(Thisform.oCallingForm.oPersist.Get(tcPersistKey), tcDefaultCode)
		
		*** JRN 12/18/2008 : if already being shown ... close it
		If Wexist(lcWindowName)
			Release Windows (lcWindowName)
		Endif
		Strtofile(lcCode, lcFileName, 0)
		
		Modify Command (lcFileName)
		
		lcCode = Filetostr(lcFileName)
		Thisform.oCallingForm.oPersist.Set(tcPersistKey, lcCode)
		
		
	ENDPROC

	PROCEDURE Init
		Lparameters lnFontSize, lnPage
		Local lnHeight, lnLeft, lnTop, lnWidth
		
		With This
			lnWidth = .Width
			lnHeight = .Height
		
			DoDefault()
		
			.Height = lnHeight
			.Width = lnWidth
		
			If lnFontSize # 8
				.ChangeFontSize.Run(lnFontSize, 8)
			EndIf 
			
			.nOriginalToolTipTimeout = _ToolTipTimeOut
			_ToolTipTimeOut = 0
			
			.Caption = ccLOC_Pref_Cap_PREFERENCES
			.Refresh()
		
			.nPage = lnPage
			
		Endwith
		
	ENDPROC

	PROCEDURE insertformorclass
		Lparameters lcCaption
		
		If This.oCallingForm.lClass
			Return Strtran(lcCaption, '<insert2>', ccLOC_Pref_Cap_FC_class)
		Else
			Return Strtran(lcCaption, '<insert2>', ccLOC_Pref_Cap_FC_Form)
		Endif
		
	ENDPROC

	PROCEDURE Load
		DoDefault()
		Thisform.oCallingForm = _oPEMEditor.oPEMEditor
		This.cfoxuserid = Thisform.oCallingForm.cfoxuserid
		
	ENDPROC

	PROCEDURE LostFocus
		
		
	ENDPROC

	PROCEDURE savehighlightsettings
		Lparameters lcSetting
		
		Local lnI
		
		If Empty (lcSetting)
			lcSetting = ''
			For lnI = 1 To Alen (Thisform.oCallingForm.aGridHighlighting)
				lcSetting = Iif(Empty(lcSetting), '', lcSetting + ', ') + Transform (Thisform.oCallingForm.aGridHighlighting(lnI))
			Next lnI
		Endif
		
		With Thisform.oCallingForm
			.oPersist.Set('GridHighLights', lcSetting)
			.InitGridHighlighting()
			.SetGridHighlighting()
			.cntGridControls.Refresh()
		Endwith
		
		Thisform.Refresh()
		
	ENDPROC

	PROCEDURE savesettings
		If PEME_IsVersion9()
			This.oPersist.Set('Dockable', This.Dockable)
		EndIf 
		
		dodefault()
		
	ENDPROC

	PROCEDURE cmdResetAllDefaults.Click
		Local lcPrompt, llUseAbbreviations, lnColumn, lnMsgBox, lnWidth
		
		lcPrompt = ccLOC_Pref_ASK_ResetAllDefaults
		lnMsgBox = Messagebox (lcPrompt, 4+32, Thisform.Caption)
		If lnMsgBox = 7
			Return
		Endif
		
		With Thisform.oCallingForm
			.oPersist.Clear()
			.GetSettings()
		
			llUseAbbreviations = .lUseAbbreviations
			lnWidth = 25 * .nFontSize / 8
			With .oGrid
				.SetColumnHeadings()
				For lnColumn = 1 To .ColumnCount
					If Not Inlist(lnColumn, 1, 9, 10)
						.Columns (lnColumn).Width = Iif(llUseAbbreviations, lnWidth, .Columns[lnColumn].nWidth)
					Endif
				Next lnColumn
				.Refresh()
			EndWith
			
			.oGrid.lFirstTime = .T.
			.oedtDefault.lFirsttime = .T.
			.oedtDescription.lFirsttime = .T.
			.SetFormAlignment()
			.SetGridHighlighting()
			.RefreshForm()
		Endwith
		
		Thisform.RefreshForm()
		
		
	ENDPROC

	PROCEDURE cmdResetAllDefaults.Init
		This.Caption = ccLOC_Pref_Cap_ResetAllDefaults
		DoDefault()
		
	ENDPROC

