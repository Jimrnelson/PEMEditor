	PROCEDURE Frame.PageGeneral.Frame.PageHighlighting.chkAbbreviations.Valid
		DoDefault()
		
		Local lnColumn, lnWidth
		With Thisform.oCallingForm
			lnWidth = 25 * .nFontSize / 8
			With .oGrid
				.SetColumnHeadings()
				For lnColumn = 1 To .ColumnCount
					If Not Inlist(lnColumn, 1, 9, 10)
						.Columns (lnColumn).Width = Iif(This.Value, lnWidth, .Columns[lnColumn].nWidth)
					Endif
				Next lnColumn
				.Refresh()
			Endwith
		Endwith
		
	ENDPROC

	PROCEDURE Frame.PageGeneral.Frame.PageHighlighting.chkGridAbove.Init
		DoDefault()
		
		With This
			If PEME_IsVersion9() and Thisform.oCallingForm.Docked
