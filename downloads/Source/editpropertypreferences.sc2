	PROCEDURE Frame.PageGeneral.Frame.pagProcessing1.cmdDefaultAccess.RightClick
		Local lcChoice, lnBar, lcPrompt
		
		lcChoice = ''
		lnBar = 0
		Thisform.CreateContextMenu("AccessContextMenu")
		
		lnBar = lnBar + 1
		Define Bar (lnBar) Of AccessContextMenu Prompt ccLOC_CMA_Edit
		On Selection Bar (lnBar) Of AccessContextMenu lcChoice = 'Edit Scalar'
		
		lnBar = lnBar + 1
		lcPrompt = Iif(Isnull(Thisform.oCallingForm.oPersist.Get("Access Code")), '\', '' ) + ccLOC_CMA_Reset
		Define Bar (lnBar) Of AccessContextMenu Prompt lcPrompt
		On Selection Bar (lnBar) Of AccessContextMenu lcChoice = 'Scalar Default'
		
		lnBar = lnBar + 1
		Define Bar (lnBar) Of AccessContextMenu Prompt "\-"
		
		lnBar = lnBar + 1
		Define Bar (lnBar) Of AccessContextMenu Prompt ccLOC_CMA_EditArray
		On Selection Bar (lnBar) Of AccessContextMenu lcChoice = 'Edit Array'
		
		lnBar = lnBar + 1
		lcPrompt = Iif(Isnull(Thisform.oCallingForm.oPersist.Get("Array Access Code")), '\', '' ) + ccLOC_CMA_ResetArray
		Define Bar (lnBar) Of AccessContextMenu Prompt lcPrompt
		On Selection Bar (lnBar) Of AccessContextMenu lcChoice = 'Array Default'
		
		Activate Popup AccessContextMenu
		Release Popups AccessContextMenu
		
		Do Case
			Case lcChoice = 'Edit Scalar'
				Thisform.EditDefaultAccess(ccLOC_Pref_ID_Default_Access, "Access Code", ccloc_Pref_ACCESS_CODE_Scalar)
			Case lcChoice = 'Scalar Default'
				Thisform.oCallingForm.oPersist.Set("Access Code", .Null.)
				
			Case lcChoice = 'Edit Array'
				Thisform.EditDefaultAccess(ccLOC_Pref_ID_Default_Access, "Array Access Code", ccloc_Pref_ACCESS_CODE_Array)
			Case lcChoice = 'Array Default'
				Thisform.oCallingForm.oPersist.Set("Array Access Code", .Null.)
		Endcase
		
		
	ENDPROC

